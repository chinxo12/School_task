@model school.Models.School

<H1>Edit School</H1>

<form asp-action="Edit" method="post">
     <div class="form-group">
        <label asp-for="SchoolId" class="control-label"></label>
        <input type="text" placeholder="@Html.DisplayFor(model => model.SchoolId)" asp-for="SchoolId" class="form-control" readonly />
        <span asp-validation-for="SchoolId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="SchoolName" class="control-label"></label>
        <input type="text" placeholder="@Html.DisplayFor(model => model.SchoolName)" asp-for="SchoolName" class="form-control" />
        <span asp-validation-for="SchoolName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Address" class="control-label"></label>
        <input type="text"  placeholder="@Html.DisplayFor(model => model.Address)" asp-for="Address"  class="form-control" />
        <span asp-validation-for="Address" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Capacity" class="control-label"></label>
        <input type="number"  placeholder="@Html.DisplayFor(model => model.Capacity)" asp-for="Capacity" class="form-control" />
        <span asp-validation-for="Capacity" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="FoundedTime" class="control-label"></label>
        <input asp-for="FoundedTime" class="form-control" readonly />
        <span asp-validation-for="FoundedTime" class="text-danger"></span>
    </div>
  
        <div class="form-group">
            <input type="submit" value="Save" class="btn btn-primary" />
            <a asp-action="Index" class="btn btn-secondary">Cancel</a>
        </div>
     
    
</form>

